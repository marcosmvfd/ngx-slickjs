{
  "name": "ngx-slickjs",
  "version": "1.4.0",
  "author": "Mehmet Erim <mehmet@mehmeterim.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mehmet-erim/ngx-slickjs.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/mehmet-erim/ngx-slickjs#readme",
  "bugs": {
    "url": "https://github.com/mehmet-erim/ngx-slickjs/issues"
  },
  "description": "ngx-slickjs is slick-carousel package for Angular 6+. ngx-slickjs can lazy load slick.js packages",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "cd src && yarn && cd ../ && ng build",
    "lint": "ng lint",
    "test": "ng test",
    "ci-test": "ng test --code-coverage --watch=false",
    "release": "cp README.md dist/ngx-slickjs/README.md && cd dist/ngx-slickjs && npm publish && cd ../..",
    "verdaccio": "yarn build && cd dist/ngx-slickjs && npm publish --registry http://localhost:4873 && cd ../..",
    "unverdaccio": "cd dist/ngx-slickjs && npm unpublish --registry http://localhost:4873 --force && cd ../.."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.1.3",
    "@angular/common": "~8.1.3",
    "@angular/compiler": "~8.1.3",
    "@angular/core": "~8.1.3",
    "@angular/forms": "~8.1.3",
    "@angular/platform-browser": "~8.1.3",
    "@angular/platform-browser-dynamic": "~8.1.3",
    "@angular/router": "~8.1.3",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.3",
    "@angular-devkit/build-ng-packagr": "~0.801.3",
    "@angular/cli": "~8.1.3",
    "@angular/compiler-cli": "~8.1.3",
    "@angular/language-service": "~8.1.3",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codecov": "^3.3.0",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.4.0"
  },
  "keywords": [
    "angular",
    "ngx-slickjs",
    "ngx",
    "slick",
    "slickjs",
    "slick-carousel",
    "carousel",
    "lazy-loading"
  ]
}
